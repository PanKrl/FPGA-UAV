//IMU_communication

#include "imu_i2c.h"

int main(){
	u8 quat_w,quat_x,quat_y,quat_z;
    IMU_begin();

	while(1){


        printf("quat_w=%04d ",quat_w);
        printf("quat_x=%04d ",quat_x);
        printf("quat_y=%04d ",quat_y);
        printf("quat_z=%04d ",quat_z);
        printf("\n");
        usleep(100);

	}

return 0;

}

