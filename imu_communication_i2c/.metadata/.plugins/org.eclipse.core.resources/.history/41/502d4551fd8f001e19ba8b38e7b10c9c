//IMU_communication

#include "imu_i2c.h"

int main(){
	u8 quat_w,quat_x,quat_y,quat_z;
    IMU_begin();
    setExtCrystalUse(TRUE);

	while(1){


        printf("quat_w=%04x ",quat_w);
        printf("quat_x=%04x ",quat_x);
        printf("quat_y=%04x ",quat_y);
        printf("quat_z=%04x ",quat_z);
        printf("\n");
        usleep(100);

	}

return 0;

}

